    363          STATUS FTL_Trim(PGADDR start, PGADDR end) {
    364            PGADDR addr;
    365            STATUS ret = STATUS_SUCCESS;
    366          
    367            for (addr = start; addr <= end; addr++) {
    368              ret = FTL_Write(addr, NULL);
    369              if (ret != STATUS_SUCCESS) {
    370                break;
    371              }
    372            }
    373          
    374            return ret;
    375          }
